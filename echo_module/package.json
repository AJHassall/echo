{
  "name": "echo_transcriber",
  "version": "0.1.0",
  "description": "wrapper for jack",
  "main": "buildjs/index.js",
    "scripts": {
    "build:addon": "cd native && cargo build --release --message-format=json > cargo.log && npx neon dist < cargo.log",
