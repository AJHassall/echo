    "src",
    "native/index.node", 
    "Cargo.toml"
  ],
  "devDependencies": {
    "@neon-rs/cli": "^0.1.82",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.11.16",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@neon-rs/load": "^0.1.82"
  },
  "keywords": [
    "jack",
    "neon",
    "rust",
    "node"
  ],
